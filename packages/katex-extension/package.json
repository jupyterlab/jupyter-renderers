{
  "name": "@jupyterlab/katex-extension",
  "version": "0.12.0",
  "description": "KaTeX math renderer for JupyterLab",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*.d.ts",
    "lib/*.js",
    "style/*.css"
  ],
  "directories": {
    "lib": "lib/"
  },
  "keywords": [
    "jupyter",
    "jupyterlab",
    "katex"
  ],
  "dependencies": {
    "@jupyterlab/application": "^0.12.0",
    "@jupyterlab/rendermime-interfaces": "^0.4.1",
    "katex": "^0.8.3"
  },
  "devDependencies": {
    "@types/katex": "^0.5.0",
    "rimraf": "^2.6.2",
    "typescript": "~2.4.2"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepack": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "jupyterlab": {
    "extension": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyter-renderers.git"
  },
  "author": "Project Jupyter",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyter-renderers/issues"
  },
  "homepage": "https://github.com/jupyterlab/jupyter-renderers"
}
