{
  "name": "@jupyterlab/geojson-extension",
  "version": "0.16.0",
  "description": "GeoJSON renderer for JupyterLab",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*.d.ts",
    "lib/*.js",
    "style/*.*"
  ],
  "directories": {
    "lib": "lib/"
  },
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "jupyterlab": {
    "mimeExtension": true
  },
  "dependencies": {
    "@jupyterlab/rendermime-interfaces": "^1.0.7",
    "@phosphor/messaging": "^1.2.2",
    "@phosphor/widgets": "^1.5.0",
    "@turf/bbox": "^6.0.1",
    "leaflet": "^1.2.0",
    "mapbox-gl": "^0.44.2"
  },
  "devDependencies": {
    "@types/mapbox-gl": "^0.44.1",
    "rimraf": "~2.6.2",
    "typescript": "~2.6.2"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepack": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyter-renderers.git"
  },
  "author": "Project Jupyter",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyter-renderers/issues"
  },
  "homepage": "https://github.com/jupyterlab/jupyter-renderers"
}
